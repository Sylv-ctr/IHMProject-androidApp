<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fac.protoim" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".home"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".profil"
            android:label="@string/title_activity_profil"
            android:parentActivityName=".home"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="fac.protoim.home" />
        </activity>
        <activity
            android:name=".listTypeFilter"
            android:label="@string/title_activity_list_type_filter"
            android:parentActivityName=".profil"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="fac.protoim.profil" />
        </activity>
        <activity
            android:name=".vueDetail"
            android:label="@string/title_activity_vue_detail"
            android:parentActivityName=".home"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="fac.protoim.home" />
        </activity>
    </application>

</manifest>
